{
    "builds": [{
        "src": "learnovation_container/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.4" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "learnovation_container/wsgi.py"
        }, 
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"}

    ]
}